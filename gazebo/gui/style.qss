{
  color: #f78e1e;
  background-color: rgba(120,120,120,255);
  border-bottom: 2px solid #f78e1e;
  font-weight: 600;
}

QTabBar#plottingTabBar::tab:hover
{
  color: #f58113;
  background-color: rgba(120,120,120,255);
}

QTabBar#plottingTabBar::tab:first:selected
{
  margin-left: 0;
}

QTabBar#plottingTabBar::tab:last:selected
{
  margin-right: 0;
}

QTabBar#plottingTabBar::tab::only-one
{
  margin-left: 0;
  margin-right: 0;
}

QTabBar#plottingTabBar
{
  background-color: rgba(120,120,120,255);
  margin: 0px;
  padding: 0px;
}

QTabBar#plottingTabBar::pane
{
  padding: 0px;
  margin: 0px;
  border: 0px;
  color: #fafafa;
}

QFrame#plotPaletteFrame
{
  background-color: #e2e2e2;
}
